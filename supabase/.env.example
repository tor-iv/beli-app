# Supabase Environment Variables
# Copy this file to .env and fill in your values

# Supabase Project URL
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
SUPABASE_URL=http://localhost:54321

# Supabase Anonymous Key (public - safe to use in frontend)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (private - NEVER expose in frontend!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Connection (for migrations/seed scripts)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# OAuth Providers (optional - configure in Supabase Dashboard)
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
# APPLE_CLIENT_ID=
# APPLE_CLIENT_SECRET=
